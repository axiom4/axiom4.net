{
  "name": "axiom4-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --subresource-integrity --inline-style-language css",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-generator-cli generate  -p supportsES6=true,withInterfaces=true,useSingleRequestParameter=true -i http://127.0.0.1:8000/openapi -g typescript-angular -o src/app/modules/core/api/v1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.4",
    "@angular/common": "^21.1.4",
    "@angular/compiler": "^21.1.4",
    "@angular/core": "^21.1.4",
    "@angular/forms": "^21.1.4",
    "@angular/platform-browser": "^21.1.4",
    "@angular/router": "^21.1.4",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "localstorage-polyfill": "^1.0.1",
    "marked": "^17.0.2",
    "mermaid": "^11.12.2",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "~21.1.3",
    "@angular/compiler-cli": "^21.1.4",
    "@angular/localize": "^21.1.4",
    "@openapitools/openapi-generator-cli": "^2.28.3",
    "@types/node": "^25.2.3",
    "@types/prismjs": "^1.26.6",
    "typescript": "~5.9.3"
  }
}
