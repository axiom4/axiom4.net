{
  "name": "axiom4-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --subresource-integrity --inline-style-language css",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-generator-cli generate  -p supportsES6=true,withInterfaces=true,useSingleRequestParameter=true -i http://127.0.0.1:8000/?format=openapi -g typescript-angular -o src/app/modules/core/api/v1",
    "serve:ssr:axiom4-web": "node dist/axiom4-web/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.1",
    "@angular/common": "^18.1.1",
    "@angular/compiler": "^18.1.1",
    "@angular/core": "^18.1.1",
    "@angular/forms": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/platform-server": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@angular/service-worker": "^18.1.1",
    "@angular/ssr": "^18.1.2",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "express": "^4.19.2",
    "localstorage-polyfill": "^1.0.1",
    "marked": "^13.0.2",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.1",
    "@angular/cli": "~18.1.1",
    "@angular/compiler-cli": "^18.1.1",
    "@angular/localize": "^18.1.1",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.14.11",
    "@types/prismjs": "^1.26.4",
    "browser-sync": "^3.0.2",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.4"
  }
