{
  "name": "axiom4-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --subresource-integrity --inline-style-language css",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-generator-cli generate  -p supportsES6=true,withInterfaces=true,useSingleRequestParameter=true -i http://127.0.0.1:8000/?format=openapi -g typescript-angular -o src/app/modules/core/api/v1",
    "dev:ssr": "ng run axiom4-web:serve-ssr",
    "serve:ssr": "node dist/axiom4-web/server/main.js",
    "build:ssr": "ng build && ng run axiom4-web:server",
    "prerender": "ng run axiom4-web:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/platform-server": "^17.3.6",
    "@angular/router": "^17.3.6",
    "@angular/service-worker": "^17.3.6",
    "@angular/ssr": "^17.3.6",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "express": "^4.19.2",
    "localstorage-polyfill": "^1.0.1",
    "marked": "^12.0.2",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "~17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@angular/localize": "^17.3.6",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.12.7",
    "@types/prismjs": "^1.26.3",
    "browser-sync": "^3.0.2",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5"
  }
}