{
  "name": "axiom4-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --subresource-integrity --inline-style-language css",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-generator-cli generate  -p supportsES6=true,withInterfaces=true,useSingleRequestParameter=true -i http://127.0.0.1:8000/openapi -g typescript-angular -o src/app/modules/core/api/v1",
    "serve:ssr:axiom4-web": "node dist/axiom4-web/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@angular/ssr": "^21.0.0",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@openapitools/openapi-generator-cli": "^2.25.2",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "express": "^5.1.0",
    "localstorage-polyfill": "^1.0.1",
    "marked": "^17.0.1",
    "mermaid": "^11.12.1",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "~21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/localize": "^21.0.0",
    "@types/express": "^5.0.5",
    "@types/jasmine": "~5.1.13",
    "@types/node": "^24.10.1",
    "@types/prismjs": "^1.26.5",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
